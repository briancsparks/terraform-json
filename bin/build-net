#!/bin/bash -e

readlink_f() { which greadlink > /dev/null && greadlink -f $1 || readlink -f $1; }
script_dir="$( cd "$(dirname "$( readlink_f "${BASH_SOURCE[0]}" )" )" && pwd )"

errcho() { echo "$@" 1>&2; }
die() { errcho "$@"; exit 1; }

echo "build-net"
exit 1

node "$1" > ./cdr0.json
node ../../foundation/scripts/json-to-tf-files.js ./cdr0.json

terraform validate && terraform apply
