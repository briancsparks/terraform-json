#!/bin/bash -e

errcho() { echo "$@" 1>&2; }
die() { errcho "$@"; exit 1; }

# https://raw.githubusercontent.com/username/path/to/script
#
# https://raw.githubusercontent.com/ username/path/to/script

RAW_LOCATION="$1"
curl -sSL "https://raw.githubusercontent.com/${RAW_LOCATION}" | grep -E -v '#!/bin/bash' | /bin/bash
